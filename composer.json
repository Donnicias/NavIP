
{
    "name": "Donnicias/NavIP",
    "description": "This is a complete package for managing inetegrations between Microsoft Dynamics Nav and laravel applications. It makes the integration abreeze.",
    "type": "laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Donnicias Mayaka",
            "email": "mayakadonnicias@gmail.com"
        }
    ],
    "keywords": ["Nav","Laravel","Integration","API"],
    "autoload": {
        "files": [
            "Support/helper.php"
        ],
        "psr-4": {
            "donnicias\\nav-ip\\": "/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Donnicias\\NavIp\\Provider\\NavIPServiceProvider"
            ],
            "aliases": {
                "Nav": "Donnicias\\NavIP\\Facades\\Nav"
            }
        }
    },
    "require": {
        "php": ">=5.6.0",
        "illuminate/support": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7|~6.2"
    }
}
